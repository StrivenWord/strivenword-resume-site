{
  "name": "strivenword-resume-site",
  "version": "1.0.0",
  "description": "My resume site",
  "main": "index.html",
  "scripts": {
    "html": "pug --pretty pug/index.pug -o ./",
    "css": "stylus stylus/design.styl -o css/design.css",
    "all": "pug --pretty pug/index.pug -o ./ && stylus stylus/design.styl -o css/design.css",
    "html-w": "pug -w --pretty pug/index.pug -o ./",
    "css-w": "stylus -w --pretty stylus/design.styl -o css/design.css",
    "all-w": "pug -w --pretty pug/index.pug -o ./ && stylus -w stylus/design.styl -o css/design.css"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StrivenWord/strivenword-resume-site.git"
  },
  "keywords": [
    "pug",
    "stylus"
  ],
  "author": "Paul Lee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StrivenWord/strivenword-resume-site/issues"
  },
  "homepage": "https://github.com/StrivenWord/strivenword-resume-site#readme"
}
